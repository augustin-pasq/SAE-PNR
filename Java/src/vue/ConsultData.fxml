<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<AnchorPane id="AnchorPane" prefHeight="720.0" prefWidth="1280.0" stylesheets="@StyleConsultData.css"
            xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="controleur.ConsultDataController">
    <opaqueInsets>
        <Insets/>
    </opaqueInsets>
    <VBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="405.0"
          prefWidth="662.0" spacing="10.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <padding>
            <Insets left="20.0" right="20.0"/>
        </padding>
        <HBox prefHeight="299.0" prefWidth="1240.0">
            <AnchorPane prefHeight="177.0" prefWidth="1240.0" HBox.hgrow="ALWAYS">
                <Button id="Back" fx:id="backButton" layoutX="459.0" layoutY="10.0" mnemonicParsing="false"
                        prefHeight="43.0" prefWidth="88.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="10.0">
                    <graphic>
                        <ImageView fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
                            <Image url="@../../data/Back.png"/>
                            <cursor>
                                <Cursor fx:constant="DEFAULT"/>
                            </cursor>
                            <viewport>
                                <Rectangle2D/>
                            </viewport>
                        </ImageView>
                    </graphic>
                </Button>
                <Button id="HomePage" fx:id="homeButton" layoutX="96.0" layoutY="10.0" mnemonicParsing="false"
                        prefHeight="43.0" prefWidth="88.0" AnchorPane.leftAnchor="96.0"
                        AnchorPane.topAnchor="10.0">
                    <graphic>
                        <ImageView fitHeight="60.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true">
                            <cursor>
                                <Cursor fx:constant="DEFAULT"/>
                            </cursor>
                            <viewport>
                                <Rectangle2D/>
                            </viewport>
                            <Image url="@../../data/Home_Page.png"/>
                        </ImageView>
                    </graphic>
                </Button>
            </AnchorPane>
        </HBox>
        <HBox alignment="CENTER" fillHeight="false" prefHeight="337.0" prefWidth="1240.0" spacing="5.0">
            <padding>
                <Insets bottom="50.0"/>
            </padding>
            <Label prefHeight="55.0" prefWidth="200.0" minWidth="235" text="Consulter" textFill="#00936e">
                <font>
                    <Font name="Berlin Sans FB" size="50.0"/>
                </font>
            </Label>
            <Label id="principal" text="des données">
                <font>
                    <Font name="Berlin Sans FB" size="50.0"/>
                </font>
            </Label>
        </HBox>
        <HBox alignment="CENTER" prefHeight="644.0" prefWidth="1240.0" spacing="50.0">
            <opaqueInsets>
                <Insets/>
            </opaqueInsets>
            <AnchorPane prefHeight="263.0" prefWidth="413.0">
                <Button id="Consulter" fx:id="consultObsButton" onAction="#consultObs" alignment="TOP_CENTER"
                        contentDisplay="BOTTOM" layoutX="65.0" layoutY="77.0" mnemonicParsing="false"
                        prefHeight="220.0" prefWidth="250.0" text="Consulter une observation"
                        AnchorPane.leftAnchor="65.0" AnchorPane.topAnchor="77.0">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <padding>
                        <Insets top="20.0"/>
                    </padding>
                    <graphic>
                        <ImageView fitHeight="124.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true">
                            <Image url="@../../data/Modify_Data.png"/>
                        </ImageView>
                    </graphic>
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Button>
            </AnchorPane>
            <AnchorPane prefHeight="310.0" prefWidth="413.0">
                <Button id="Generer" fx:id="generateChartButton" onAction="#generateChart" alignment="TOP_CENTER"
                        contentDisplay="BOTTOM" layoutX="65.0" layoutY="77.0" mnemonicParsing="false"
                        prefHeight="220.0" prefWidth="250.0" text="Générer un graphique">
                    <opaqueInsets>
                        <Insets/>
                    </opaqueInsets>
                    <padding>
                        <Insets top="20.0"/>
                    </padding>
                    <graphic>
                        <ImageView fitHeight="124.0" fitWidth="120.0" pickOnBounds="true" preserveRatio="true">
                            <Image url="@../../data/Stat.png"/>
                        </ImageView>
                    </graphic>
                    <font>
                        <Font size="20.0"/>
                    </font>
                </Button>
            </AnchorPane>
        </HBox>
    </VBox>
</AnchorPane>
